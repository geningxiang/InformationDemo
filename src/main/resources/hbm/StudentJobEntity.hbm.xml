<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.caimao.information.entity.StudentJobEntity" table="STUDENT_JOB" schema="information">
        <id name="studentId" column="STUDENT_ID"/>
        <property name="articleListMd5" column="ARTICLE_LIST_MD5"/>
        <property name="articleListSortEvent" column="ARTICLE_LIST_SORTEVENT"/>
        <property name="articleListData" column="ARTICLE_LIST_DATA"/>
        <property name="articleListEvent" column="ARTICLE_LIST_EVENT"/>
        <property name="articleDetailMd5" column="ARTICLE_DETAIL_MD5"/>
        <property name="articleDetailData" column="ARTICLE_DETAIL_DATA"/>
        <property name="matchListMd5" column="MATCH_LIST_MD5"/>
        <property name="matchListData" column="MATCH_LIST_DATA"/>
        <property name="matchListEvent" column="MATCH_LIST_EVENT"/>
        <property name="matchDetailMd5" column="MATCH_DETAIL_MD5"/>
        <property name="matchDetailData" column="MATCH_DETAIL_DATA"/>
        <property name="createTime" column="CREATE_TIME"/>
    </class>
</hibernate-mapping>